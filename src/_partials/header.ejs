<header class="c-header">
  <div class="animations animationFadeIn">
    <div class="animation-1">
      <img src="../_public/assets/img/animation-1.png" alt="animations-item" />
    </div>
    <div class="animation-2">
      <img src="../_public/assets/img/animation-2.png" alt="animations-item" />
    </div>
    <div class="animation-3">
      <img src="../_public/assets/img/animation-3.png" alt="animations-item" />
    </div>
    <div class="animation-4">
      <img src="../_public/assets/img/animation-4.png" alt="animations-item" />
